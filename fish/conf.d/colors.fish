# Set syntax highlighting colors to match Everblush
set -g fish_color_normal ffffff
set -g fish_color_command 7dcfff
set -g fish_color_quote 9aa5ce
set -g fish_color_redirection 9aa5ce
set -g fish_color_end 9aa5ce
set -g fish_color_error f7768e
set -g fish_color_param 9aa5ce
set -g fish_color_comment 676E95
set -g fish_color_selection --background=383e5a
set -g fish_color_search_match --background=383e5a
set -g fish_color_operator 7dcfff
set -g fish_color_escape 7dcfff
set -g fish_color_autosuggestion 676E95
